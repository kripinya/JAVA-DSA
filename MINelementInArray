public class findMinNo {
    public static int findMinimum(int arr[]){
       int n = arr.length;
        int min = arr[0];
        for(int i = 1; i<n; i++){
            if(arr[i]<min)
            min = arr[i];
        }
        return min;
    }
    public static void main(String[] args){
        int arr[] = {5, 2, 8, 12, 3};
        System.out.println("Minimum element is " + findMinimum(arr) + " in the given array");
    }
}
